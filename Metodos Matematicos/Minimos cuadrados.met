grasas <- read.table("http://www.uam.es/joser.berrendero/datos/EdadPesoGrasas.txt", 
    header = TRUE)
names(grasas)
regresion <- lm(grasas~ edad, data = grasas)
plot(grasas$edad, grasas$grasas, xlab =" Edad" , ylab="Grasas" )
mcm<-abline(regresion)


